<h1>Editing account_group</h1>

<% form_for(@account_group) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @account_group %> |
<%= link_to 'Back', account_groups_path %>